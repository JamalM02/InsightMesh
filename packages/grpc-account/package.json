{
  "name": "@insightmesh/grpc-account",
  "version": "1.0.0",
  "description": "TypeScript gRPC service using nice-grpc",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc && tsc-alias",
    "start": "node dist/app.js",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node -r tsconfig-paths/register' src/app.ts | pino-pretty",
    "dev:windows": "npx nodemon --watch src --ext ts,json --exec \"npx ts-node -r tsconfig-paths/register src/app.ts\" | npx pino-pretty",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate:dev": "npx prisma migrate dev",
    "prisma:reset": "npx prisma migrate reset",
    "proto:generate": "node ../node-common/scripts/generate-proto.js",
    "postinstall": "npm run client:generate",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' --fix",
    "format": "prettier --write 'src/**/*.{ts,js,json}'",
    "client:generate": "node ../node-common/scripts/generate-client.js",
    "proto:watch": "chokidar service.proto -c 'npm run client:generate'"
  },
  "dependencies": {
    "@clickhouse/client": "^1.11.1",
    "@insightmesh/node-common": "*",
    "@prisma/client": "^6.5.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "encrypt-tools": "^1.0.4",
    "kafkajs": "^2.2.4",
    "long": "^5.2.3",
    "nice-grpc": "^2.1.0",
    "nice-grpc-common": "^2.0.2",
    "protobufjs": "^7.2.3",
    "stripe": "^18.0.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "grpc-tools": "^1.12.4",
    "nodemon": "^3.0.1",
    "pino-pretty": "^13.0.0",
    "prettier": "^3.2.5",
    "prisma": "^6.5.0",
    "ts-node": "^10.9.1",
    "ts-proto": "^1.181.2",
    "tsc-alias": "^1.8.13",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  }
}
